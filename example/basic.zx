// This a simple example

// built in library
// math is file ?
// plus function ?
im math
im math::{ pow, PI }

// program entry point
fn main() {
    // & is std library
    &::print("hello world")

    // variable declaration example
    var example: i32 = 10

    // call function example
    var number = pow(10, 5) // number equal 10^5
    math::abs(-32) // 32
}
